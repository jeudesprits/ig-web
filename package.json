{
  "name": "ig-web",
  "version": "1.0.0",
  "description": "ig-web",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run serve",
    "debug": "npm run build && npm run watch-debug",
    "build": "npm run build-ts && npm run lint",
    "serve": "node dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "build-ts": "tsc",
    "lint": "npm run lint-ts && npm run lint-prettier",
    "lint-ts": "tslint -c tslint.json -p tsconfig.json",
    "lint-prettier": "prettier --check --config .prettierrc --ignore-path .gitignore \"src/**/*.ts\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "serve-debug": "nodemon --inspect dist/server.js",
    "format": "npm run format-ts && npm run format-prettier",
    "format-ts": "tslint --fix -c tslint.json -p tsconfig.json",
    "format-prettier": "prettier --config .prettierrc --ignore-path .gitignore --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeudesprits/ig-web.git"
  },
  "author": "jeudesprits",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jeudesprits/ig-web/issues"
  },
  "homepage": "https://github.com/jeudesprits/ig-web#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv-safe": "^6.1.0",
    "express": "^4.17.1",
    "form-data": "^2.5.0",
    "maraquia": "^0.2.2",
    "mongodb": "^3.2.7",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.0",
    "puppeteer": "^1.18.1",
    "readline-sync": "^1.4.9",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/dotenv-safe": "^5.0.3",
    "@types/express": "^4.17.0",
    "@types/form-data": "^2.2.1",
    "@types/mongodb": "^3.1.28",
    "@types/node": "^12.6.1",
    "@types/node-cron": "^2.0.2",
    "@types/node-fetch": "^2.3.7",
    "@types/puppeteer": "^1.12.4",
    "@types/readline-sync": "^1.4.3",
    "@types/winston": "^2.4.4",
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.3"
  }
}
