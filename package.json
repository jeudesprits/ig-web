{
  "name": "ig-web",
  "version": "1.0.0",
  "description": "ig-web",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/server.js",
    "build-ts": "tsc",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeudesprits/ig-web.git"
  },
  "author": "jeudesprits",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jeudesprits/ig-web/issues"
  },
  "homepage": "https://github.com/jeudesprits/ig-web#readme",
  "dependencies": {
    "dotenv-safe": "^6.1.0",
    "form-data": "^2.5.0",
    "maraquia": "^0.2.2",
    "mongodb": "^3.3.1",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.0",
    "puppeteer": "^1.19.0",
    "puppeteer-extra": "^2.1.3",
    "puppeteer-extra-plugin-stealth": "^2.2.2",
    "readline-sync": "^1.4.10",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@types/dotenv-safe": "^5.0.3",
    "@types/form-data": "^2.5.0",
    "@types/mongodb": "^3.3.1",
    "@types/node": "^12.7.2",
    "@types/node-cron": "^2.0.2",
    "@types/node-fetch": "^2.5.0",
    "@types/puppeteer": "^1.19.1",
    "@types/readline-sync": "^1.4.3",
    "@types/winston": "^2.4.4",
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.1",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.3"
  }
}
